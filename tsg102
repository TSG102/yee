import math
import cmath

a = float(input("input real part "))
b = float(input("input complex part "))
c = a.real + 1j*b
d = 1
while d < 10000:
       d = d + 1
       c = c * c
if a > 1 or a < 1 and abs(b)>1:
    print("the number is not on the set")
elif a < 1 or a > 1 and abs(b) < 1:
    print("the number is on the set")
